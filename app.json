{
  "expo": {
    "name": "Deepify",
    "slug": "Deepify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/m.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/m.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepak7.Deepify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/m.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/m.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e9643c08-74b5-436a-b963-f9b55ae37bab"
      }
    }
  }
}